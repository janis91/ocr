<?xml version="1.0" encoding="utf-8" ?>
<database xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/pear/MDB2_Schema/master/docs/MDB.xsd">
	<name>*dbname*</name>
	<create>true</create>
	<overwrite>false</overwrite>
	<!-- CREATE TABLE ocr_jobs ( id INT(8) UNSIGNED NOT NULL PRIMARY KEY AUTO_INCREMENT, 
		status TEXT NOT NULL, source TEXT NOT NULL, target TEXT NOT NULL, temp_file 
		TEXT NOT NULL, type TEXT NOT NULL, user_id TEXT NOT NULL, error_displayed 
		BOOLEAN NOT NULL, original_filename TEXT NOT NULL, log TEXT NOT NULL ) ENGINE=InnoDB 
		DEFAULT CHARSET=utf8mb4; -->
	<table>
		<name>*dbprefix*ocr_jobs</name>
		<declaration>
			<field>
				<name>id</name>
				<type>integer</type>
				<notnull>true</notnull>
				<autoincrement>true</autoincrement>
				<unsigned>true</unsigned>
				<length>8</length>
			</field>
			<field>
				<name>status</name>
				<type>text</type>
				<length>10</length>
				<default></default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>source</name>
				<type>text</type>
				<length>255</length>
				<default></default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>target</name>
				<type>text</type>
				<length>255</length>
				<default></default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>temp_file</name>
				<type>text</type>
				<length>255</length>
				<default></default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>type</name>
				<type>text</type>
				<length>5</length>
				<default></default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>user_id</name>
				<type>text</type>
				<length>200</length>
				<default></default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>error_displayed</name>
				<type>boolean</type>
				<default>false</default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>original_filename</name>
				<type>text</type>
				<length>100</length>
				<default></default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>error_log</name>
				<type>text</type>
				<length>500</length>
				<default></default>
				<notnull>true</notnull>
			</field>
			<field>
				<name>replace</name>
				<type>boolean</type>
				<default>false</default>
				<notnull>true</notnull>
			</field>
		</declaration>
	</table>
</database>