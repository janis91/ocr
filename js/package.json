        "coverage": "cat ./coverage/report-lcov/lcov.info | codacy-coverage --token $codacy_api_key --language typescript",
        "lint": "tslint -c ../tslint.json -p tsconfig.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/janis91/ocr.git"
    },
