        "karma-spec-reporter": "0.0.32",
        "karma-typescript": "^4.1.1",
        "rimraf": "^2.7.1",
        "tslint": "^5.20.1",
        "tslint-loader": "^3.5.4",
        "typescript": "^3.7.4",
        "webpack": "^4.41.5",
