    },
    "homepage": "https://github.com/janis91/ocr#readme",
    "private": true,
    "devDependencies": {
        "@types/jasmine": "^3.5.0",
        "@types/jasmine-ajax": "^3.3.0",
        "@types/jquery": "^3.3.31",
        "@types/pdfjs-dist": "^0.1.2",
        "codacy-coverage": "^3.4.0",
        "handlebars": "^4.7.2",
        "handlebars-loader": "^1.7.1",
        "jasmine-ajax": "^4.0.0",
        "jasmine-core": "^3.5.0",
        "karma": "^4.4.1",
