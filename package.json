{
  "name": "ocr",
  "version": "6.0.0",
  "private": true,
  "description": "Nextcloud OCR (optical character recoginition) processing for images and PDF with tesseract-js",
  "author": "Janis Koehr <janiskoehr@icloud.com>",
  "scripts": {
    "build": "vue-cli-service build && npm run postbuild",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "coverage": "cat ./tests/js/unit/coverage/lcov.info | codacy-coverage --token $codacy_api_key --language typescript",
    "lint": "vue-cli-service lint",
    "postbuild": "rimraf js && rimraf css && ncp dist/js js && ncp dist/css css"
  },
  "dependencies": {
    "@nextcloud/auth": "^1.2.1",
    "@nextcloud/axios": "^1.3.1",
    "@nextcloud/l10n": "^1.0.1",
    "@nextcloud/router": "^1.0.0",
    "@nextcloud/vue": "^1.2.8",
    "core-js": "^3.4.4",
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@types/jquery": "^3.3.31",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-e2e-cypress": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.1",
    "typescript": "~3.5.3",
    "vue-template-compiler": "^2.6.10"
  },
  "bugs": {
    "url": "https://github.com/janis91/ocr/issues"
  },
  "homepage": "https://github.com/janis91/ocr#readme",
  "keywords": [
    "ocr"
  ],
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/janis91/ocr.git"
  }
}
